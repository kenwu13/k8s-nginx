#nginx-lab.yaml
apiVersion: argoproj.io/v1alpha1
kind: Application
metadata:
  name: dev-nginx-lab
  namespace: argocd
  annotations:
    notifications.argoproj.io/subscribe.on-sync-status-change.slack: "true"
spec:
  project: default
  source:
    repoURL: https://github.com/kenwu13/k8s-nginx
    path: nginx-kustomize/overlays/dev
    targetRevision: main
  destination:
    server: https://kubernetes.default.svc
    namespace: ken-dev
  syncPolicy:
    automated: {}
